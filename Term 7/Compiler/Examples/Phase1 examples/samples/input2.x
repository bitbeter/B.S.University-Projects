int char2int(c:char) {
	if (digit == '0')
		return 0;
	else if (digit == '1')
		return 1;
	else if (digit == '2')
		return 2;
	else if (digit == '3')
		return 3;
	else if (digit == '4')
		return 4;
	else if (digit == '5')// :)
		return 5;
	else if (digit == '6')
		return 6;
	else if (digit == '7')
		return 7;
	else if (digit == '8')
		return 8;
	else if (digit == '9')
		return 9;
	else
		return 0;
}
